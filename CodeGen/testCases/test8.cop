#Given a circle x^2+y^2=9 and a line x+y=3. Find at how many points the given line meets the circle.

main -> int
<<
    circle cir : 1, 1, 0, 0, 0, -9
    line l: 0, 0, 0, 1, 1, -6
    point p1
    p1 = centre(cir)
    point p2 = intersec_pt( l , l1 )
    double dis = ((x(p1) - x(p2)) + (y(p1) - y(p2)))
    double rad = radius (cir)
    if(rad>dis)                 #incomplete function
    <<
        temp = temp + 1          #temp not defined
    >>
    else
    <<
        temp = temp - 1
    >>
    exit: 0
>>
